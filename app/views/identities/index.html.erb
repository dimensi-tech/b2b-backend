<p id="notice"><%= notice %></p>

<h1>Identities</h1>

<table>
  <thead>
    <tr>
      <th>Nik</th>
      <th>Full name</th>
      <th>Birth place</th>
      <th>Birth date</th>
      <th>Gender</th>
      <th>Address</th>
      <th>Hamlet</th>
      <th>Neighbourhood</th>
      <th>Urban village</th>
      <th>Sub district</th>
      <th>District</th>
      <th>Province</th>
      <th>City</th>
      <th>Religion</th>
      <th>Martial status</th>
      <th>Job</th>
      <th>Nationality</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @identities.each do |identity| %>
      <tr>
        <td><%= identity.nik %></td>
        <td><%= identity.full_name %></td>
        <td><%= identity.birth_place %></td>
        <td><%= identity.birth_date %></td>
        <td><%= identity.gender %></td>
        <td><%= identity.address %></td>
        <td><%= identity.hamlet %></td>
        <td><%= identity.neighbourhood %></td>
        <td><%= identity.urban_village %></td>
        <td><%= identity.sub_district %></td>
        <td><%= identity.district %></td>
        <td><%= identity.province_id %></td>
        <td><%= identity.city_id %></td>
        <td><%= identity.religion %></td>
        <td><%= identity.martial_status %></td>
        <td><%= identity.job %></td>
        <td><%= identity.nationality %></td>
        <td><%= link_to 'Show', identity %></td>
        <td><%= link_to 'Edit', edit_identity_path(identity) %></td>
        <td><%= link_to 'Destroy', identity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Identity', new_identity_path %>
